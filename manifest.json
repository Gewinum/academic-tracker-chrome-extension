{
  "manifest_version": 3,
  "name": "BTU+ Plus | Reworked pages - GPA Calculator & Barrier Progress Bar",
  "version": "0.3.0",
  "description": "BTU exclusive classroom with GPA calculation and improved barrier progress bar visualization for current semester courses",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "minimum_chrome_version": "116",
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://classroom.btu.edu.ge/ge/student/me/courses",
        "https://classroom.btu.edu.ge/ge/student/me/course/scores/*/*",
        "https://classroom.btu.edu.ge/ge/student/card",
        "https://classroom.btu.edu.ge/ge/student/me/schedule"
      ]
    }
  ]
}
