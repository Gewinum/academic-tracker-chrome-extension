{
  "manifest_version": 3,
  "name": "BTU Reworked | GPA, Barrier Progress Bar",
  "version": "0.2.0",
  "description": "BTU Exclusive: new GPA Viewer for Current Semester's Completed Courses and Overhauled Progress Bar",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://classroom.btu.edu.ge/ge/student/me/courses",
        "https://classroom.btu.edu.ge/ge/student/me/course/scores/*/*"
      ]
    }
  ]
}
